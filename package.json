{
  "name": "yocto-pm2-mongodb-dump",
  "version": "1.0.3",
  "description": "A pm2 module to process automatic dump on your mongodb database",
  "main": "app.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/yoctore/yocto-pm2-mongodb-dump.git"
  },
  "author": {
    "name": "ROBERT Mathieu",
    "email": "mathieu@yocto.re",
    "url": "http://www.yocto.re"
  },
  "license": "Apache-2.0",
  "keywords": [
    "pm2",
    "mongo",
    "probe",
    "metric",
    "dump",
    "save"
  ],
  "config": {
    "ip": "127.0.0.1",
    "port": 27017,
    "username": "",
    "password": "",
    "ssl": false,
    "authDB": "",
    "sslCa": "",
    "sslCert": "",
    "sslKey": "",
    "sslValidate": false,
    "checkServerIdentity": false,
    "auth": false,
    "authenticationMechanism": "SCRAM-SHA-1",
    "refreshDelay": 3600000,
    "database": "r2do",
    "mode": "keymetrics",
    "compress": true,
    "dateFormat": "YYYY-MM-DD__HHmmss",
    "dumpOnstartUp": false,
    "archiveLimit": 48,
    "outPath": "",
    "quiet": false
  },
  "dependencies": {
    "async": "^2.0.1",
    "bytes": "^2.4.0",
    "fs-extra": "^0.30.0",
    "lodash": "^4.14.0",
    "moment": "^2.14.1",
    "mongodb": "^2.2.5",
    "pmx": "^0.6.2",
    "shelljs": "^0.7.3"
  },
  "devDependencies": {
    "grunt": "^1.0.1",
    "yocto-hint": "^1.3.7"
  }
}
